

String password = "Indraraj";
String password_one;
void setup()
{
  Serial.begin(9600);

}

void loop()
{
  
  
  
  if(Serial.available())
  {
  password_one= Serial.readString();
  
  
  if (password_one == password) {
    Serial.println("Correct Password");
  } 
    
    else {
    Serial.println("Incorrect Password");
  }
  }  
  delay(10);
}
