#include <stdio.h>

void setup() 
{
  Serial.begin(9800);
  String password= String ("789");      

}


void loop() {
  Serial.println("Enter password");    
  String password= String ("789");      
  String a= Serial.readString();
  if (a!= password) 
    {
      Serial.println("wrong password."); 
    }
  else 
    {
      Serial.println("Password is matching.");
    }
}
