int out1 = 7;
int out2 = 6;
int out3 = 5;
int out4 = 4;
void setup()
{
  pinMode(7,OUTPUT);
  pinMode(6,OUTPUT);
  pinMode(5,OUTPUT);
  pinMode(4,OUTPUT);
}

void loop()
{
    analogWrite(out1,analogRead(A0));
    analogWrite(out2,analogRead(A1));
    analogWrite(out3,analogRead(A3));
    analogWrite(out4,analogRead(A4)); 
}
